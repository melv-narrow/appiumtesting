<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="eCommerce Test Suite">
    <listeners>
        <listener class-name="utils.Listeners"/>
    </listeners>
    <test name="eCommerce Tests">
        <classes>
            <class name="eCommerceTests">
                <methods>
                    <include name="FillForm"/>
                    <include name="FillFormWithoutName"/>
                    <include name="ScrollToAddProductToCart"/>
                    <include name="SumOfProductsInCart"/>
                    <include name="PurchaseProductHybrid"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>